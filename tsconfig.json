{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./"],
      "~/types/*": ["./types/*"]
    }
  },
  "types": ["@types/node", "@nuxt/types", "@nuxtjs/axios", "@nuxtjs/auth"],
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ]
}
